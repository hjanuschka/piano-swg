# Piano API Configuration
# Required: Your Piano AID from your Piano account
PIANO_AID=your_aid_here

# Required: Your Piano API token
PIANO_API_TOKEN=your_api_token_here

# Required: Your Piano JWT secret for token signing
PIANO_JWT_SECRET=your_jwt_secret_here

# Required: Your Piano REST key
PIANO_REST_KEY=your_rest_key_here

# Required: Your Piano type (sandbox, id, or id-eu)
PIANO_TYPE=sandbox

# Required: Your Piano private key
PIANO_PRIVATE_KEY=your_private_key_here

# Google API Configuration
# Required: Your Google API credentials JSON (service account key)
GOOGLE_AUTH_JSON={"type": "service_account", "project_id": "your-project-id", ...}

# Required: Your Google SwG publication ID
PUBLICATION_ID=your_publication_id_here

# Required: Your SwG product ID
PRODUCT_ID=your_product_id_here

# Server Configuration
# Optional: Server address (default: :8080)
SERVER_ADDRESS=:8080

# Optional: Enable debug logging (default: false)
DEBUG=false

# Required: Domain for cookie setting (e.g., .example.com)
COOKIE_DOMAIN=.example.com

# Optional: Cookie expiration duration (default: 720h)
COOKIE_EXPIRE=720h

# Optional: Trinity empty ID (default: 00000000-0000-0000-0000-000000000000)
TRINITY_EMPTY_ID=00000000-0000-0000-0000-000000000000 